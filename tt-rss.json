{
    "tt-rss": {
        "description": "Tiny Tiny RSS web-based news feed (RSS/Atom) reader and aggregator",
        "version": "1.0.0",
        "website": "https://tt-rss.org/gitlab/fox/tt-rss",
        "more_info": "",
        "containers": {
            "tt-rss": {
                "image": "x86dev/docker-ttrss",
                "tag": "latest",
                "launch_order": 1,
                "ports": {
                    "80": {
                        "description": "tt-rss Web port. Suggested default: 8080",
                        "host_default": 8080,
                        "label": "Web port",
                        "protocol": "tcp",
                        "ui": true
                    }
                },
                "volumes": {
                    "/var/www/ttrss": {
                        "description": "Choose a Share for the configuration files. Eg: create a Share called tt-rss-config for this purpose alone.",
                        "label": "Config Storage",
                        "min_size": 1073741824
                    }
                }
            }
        }
    }
}
